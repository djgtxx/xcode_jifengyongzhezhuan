				  <string>backToTown</string>
				  <key>luaScript</key>              <!--lua脚本文件，需要使用相对路径，此条可省略-->
				  <string></string>
				  <key>isVisible</key>              <!--初始时是否可见，1为可见，0为不可见，默认为1-->
