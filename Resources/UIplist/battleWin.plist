				    <key>AspriteManager</key>
				    <dict>
					  <key>ipad</key>
					  <string>UI/ui_win.bin</string>
					  <key>iphone</key>
					  <string>UI/ui_win.bin</string>
				    </dict>
				  </dict>
				  <key>useASposition</key>          <!--是否使用Asprite中的坐标，默认为0，使用的坐标为clickedTexture的坐标-->
