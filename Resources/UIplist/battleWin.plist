					<key>normalTexture</key>        <!--未被选中时的贴图名，需要使用宏定义中的贴图名-->
					<string>map_ui_win_FRAME_BUTTON_BACKTOTOWN</string>
					<key>clickedTexture</key>       <!--选中时的贴图名，需要使用宏定义中的贴图名-->
